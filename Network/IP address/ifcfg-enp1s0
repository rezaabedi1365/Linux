#vi /etc/sysconfig/network-scripts/ifcfg-enp1s0
    TYPE=Ethernet
    PROXY_METHOD=none
    BROWSER_ONLY=no
    BOOTPROTO=none
    DEFROUTE=yes
    IPV4_FAILURE_FATAL=yes
    IPV6INIT=yes
    IPV6_AUTOCONF=yes
    IPV6_DEFROUTE=yes
    IPV6_FAILURE_FATAL=no
    IPV6_ADDR_GEN_MODE=stable-privacy
    NAME=enp1s0
    UUID=11b2bf56-a0d8-4282-bf6f-d59be143f8e9
    DEVICE=enp1s0
    ONBOOT=yes
    IPADDR=192.168.0.100
    PREFIX=24
    GATEWAY=192.168.0.1
    DNS1=192.168.0.100
    IPV6_PRIVACY=no
